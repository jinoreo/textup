{
    "rewrites": [
      {
        "source": "/textup",
        "destination": "https://textup.vercel.app"
      },
      {
        "source": "/textup/:path*",
        "destination": "https://textup.vercel.app/:path*"
      },
      {
        "source": "/citeease",
        "destination": "https://cite-ease.vercel.app"
      },
      {
        "source": "/citeease/:path*",
        "destination": "https://cite-ease.vercel.app/:path*"
      },
      {
        "source": "/capypress",
        "destination": "capy-press.vercel.app"
      },
      {
        "source": "/capypress/:path*",
        "destination": "https://capy-press.vercel.app/:path*"
      },
      {
        "source": "/",
        "destination": "https://textup.vercel.app"
      }
    ],
    "headers": [
      {
        "source": "/citeease/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }