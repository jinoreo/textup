{
    "rewrites": [
      {
        "source": "/textup",
        "destination": "https://textup.vercel.app"
      },
      {
        "source": "/textup/:path*",
        "destination": "https://textup.vercel.app/:path*"
      },
      {
        "source": "/citeease",
        "destination": "https://cite-ease.vercel.app"
      },
      {
        "source": "/citeease/:path*",
        "destination": "https://cite-ease.vercel.app/:path*"
      },
      {
        "source": "/capypress",
        "destination": "https://capy-press.vercel.app"
      },
      {
        "source": "/capypress/:path*",
        "destination": "https://capy-press.vercel.app/:path*"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { 
            "key": "Access-Control-Allow-Origin", 
            "value": "*" 
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/capypress/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "trailingSlash": false
}