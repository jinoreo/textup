{
  "routes": [
    {
      "src": "^/textup$",
      "dest": "/index.html"
    },
    {
      "src": "/citeease(.*)",
      "dest": "https://cite-ease.vercel.app$1"
    },
    {
      "src": "/www.vansun.top(.*)",
      "status": 308,
      "headers": { "Location": "$1" }
    },
    {
      "src": "^/$",
      "status": 404
    }
  ]
} 