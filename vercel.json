{
    "routes": [
      {
        "src": "^/$",
        "dest": "https://textup.vercel.app"
      },
      {
        "src": "^/textup$",
        "dest": "https://textup.vercel.app"
      },
      {
        "src": "^/textup/(.*)",
        "dest": "https://textup.vercel.app/$1"
      },
      {
        "src": "^/citeease$",
        "dest": "https://cite-ease.vercel.app"
      },
      {
        "src": "^/citeease/(.*)",
        "dest": "https://cite-ease.vercel.app/$1"
      }
    ],
    "cleanUrls": true
  }