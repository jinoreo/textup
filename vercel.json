{
    "routes": [
      {
        "src": "^/textup$",
        "dest": "/index.html"
      },
      {
        "src": "^/citeease$",
        "dest": "https://cite-ease.vercel.app"
      },
      {
        "src": "^/citeease/(.*)$",
        "dest": "https://cite-ease.vercel.app/$1"
      },
      {
        "src": "^/(?!textup$|citeease(?:/|$)).*$",
        "status": 404
      }
    ]
  }